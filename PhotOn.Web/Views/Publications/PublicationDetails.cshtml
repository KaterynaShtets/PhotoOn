@model PhotOn.Web.ViewModels.PublicationViewModel
@{
    ViewData["Title"] = "Publication";
    ViewBag.Title = Model.Title;
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h1>@Model.Title</h1>
<h3>Likes: @Model.LikeCount</h3>
<h3>Season: @Model.Season</h3>
<h3>Description: @Model.TextDescription</h3>
<h3>X: @Model.coordX</h3>
<h3>Y: @Model.coordY</h3>
<h3>Price: @Model.Price</h3>